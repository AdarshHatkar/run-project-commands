{
  "name": "run-project-commands",
  "version": "1.0.0",
  "description": "A CLI tool to quickly run scripts from package.json",
  "main": "bin/index.js",
  "bin": {
    "rpc": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/run-project-commands.git"
  },
  "homepage": "https://github.com/yourusername/run-project-commands#readme",
  "bugs": {
    "url": "https://github.com/yourusername/run-project-commands/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "lint": "tsc --noEmit",
    "clean": "rimraf bin dist",
    "rebuild": "npm run clean && npm run build",
    "start": "node bin/index.js",
    "package": "pkg .",
    "publish-npm": "npm publish",
    "install-global": "npm install -g ."
  },
  "keywords": [
    "cli",
    "npm",
    "scripts",
    "runner",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "enquirer": "^2.4.1",
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^4.9.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "pkg": {
    "scripts": "bin/**/*.js",
    "targets": [
      "node18-win-x64",
      "node18-macos-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  }
}
